# App
# used in apps/api-laravel/config/app.php
APP_ENV=local
# used in apps/api-laravel/config/app.php
APP_DEBUG=false

# URLs
# used in apps/api-laravel/config/app.php
APP_URL=http://localhost:8080
# used in apps/api-laravel/config/app.php
WEBAPP_URL=http://localhost:5173
# used in apps/bot-node/src/index.ts, apps/bot-worker/src/index.ts
LARAVEL_API_URL=http://api:8080
# used in apps/webapp/src/lib/api.ts
VITE_API_URL=http://localhost:8080
# used in apps/bot-node/src/index.ts
BOT_WEBHOOK_URL=
# used in apps/bot-node/src/index.ts
BOT_WEBHOOK_PATH=/bot/webhook

# Database
# used in docker-compose.yml
POSTGRES_DB=app
# used in docker-compose.yml
POSTGRES_USER=app
# used in docker-compose.yml
# REQUIRED SECRET
POSTGRES_PASSWORD=

# Telegram (Bot/WebApp)
# used in apps/bot-node/src/index.ts, apps/bot-worker/src/index.ts, apps/api-laravel/app/Services/TelegramInitDataVerifier.php
# REQUIRED SECRET
BOT_TOKEN=
# used in apps/bot-node/src/index.ts, apps/bot-worker/src/index.ts, apps/api-laravel/app/Http/Middleware/BotServiceTokenMiddleware.php
# REQUIRED SECRET
BOT_SERVICE_TOKEN=
# used in apps/bot-worker/src/index.ts
WORKER_THROTTLE_MS=350
